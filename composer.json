{
    "name": "surfnet/stepup-bundle",
    "description": "A Symfony2 bundle that holds shared code and framework integration for all Step-up applications.",
    "keywords": [
        "stepup",
        "suaas",
        "surfnet"
    ],
    "license": "Apache-2.0",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Surfnet\\StepupBundle\\": "src"
        }
    },
    "require": {
        "php": "~5.4",
        "ext-openssl": "*",
        "graylog2/gelf-php": "~1.1",
        "guzzlehttp/guzzle": "~4",
        "monolog/monolog": "~1.11",
        "sensio/framework-extra-bundle": "~3",
        "symfony/config": "~2",
        "symfony/dependency-injection": "~2",
        "symfony/http-kernel": "~2",
        "symfony/validator": "~2"
    },
    "require-dev": {
        "ibuildings/qa-tools": "~1.1",
        "mockery/mockery": "0.9.*",
        "liip/rmt": "1.1.*"
    }
}
